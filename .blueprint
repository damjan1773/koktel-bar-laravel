models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable default:NULL', password: string, remember_token: 'string:100 nullable default:NULL' }
    Korisnik: { table: users, ime: string, prezime: string, uloga: string, relationships: { hasMany: Porudzbina } }
    Koktel: { naziv: string, cena: decimal, opis: 'text nullable', relationships: { belongsToMany: Sastojak } }
    Sastojak: { naziv: string, kolicina: 'decimal default:0', jedinica: string, relationships: { belongsToMany: Koktel } }
    Porudzbina: { broj_stola: integer, status: 'string default:created', napomena: 'text nullable', korisnik_id: 'id foreign:users', relationships: { belongsTo: Korisnik, hasMany: StavkaPorudzbine } }
    StavkaPorudzbine: { porudzbina_id: 'id foreign:porudzbinas', koktel_id: 'id foreign:koktels', kolicina: 'integer default:1', jedinicna_cena: decimal, relationships: { belongsTo: 'Porudzbina, Koktel' } }
